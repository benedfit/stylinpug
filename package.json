{
  "name": "stylperjade",
  "version": "0.0.1",
  "description": "Checks Stylus against Jade for orphaned classes",
  "author": "Ben Edwards <hello@benedfit.com>",
  "license": "BSD",
  "tags": [
    "jade",
    "stylus",
    "uncss"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:benedfit/stylperjade"
  },
  "publishConfig": {
