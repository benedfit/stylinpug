{
  "name": "stylperjade",
  "version": "1.0.0",
  "description": "Checks Jade against CSS, and vice versa, for unused and blacklisted classes",
  "author": "Ben Edwards <hello@benedfit.com>",
  "license": "ISC",
  "keywords": [
    "uncss",
    "unusedcss",
    "optimize",
    "optimise",
    "unused",
    "selector",
    "sourcemap",
    "css",
    "stylus",
    "jade",
    "html"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:benedfit/stylperjade"
  },
  "publishConfig": {
    "registry": "http://registry.npmjs.org"
  },
  "main": "./lib/stylperjade",
  "bin": {
    "stylperjade": "./bin/stylperjade"
  },
  "scripts": {
    "lint": "jshint .",
    "checkStyle": "jscs .",
    "wrap": "rm -f npm-shrinkwrap.json; npm prune && npm install && npm shrinkwrap && pliers cleanShrinkwrap",
    "pretest": "npm run lint && npm run checkStyle",
    "test": "istanbul cover _mocha -- -R spec",
    "posttest": "istanbul check-coverage && rm -rf coverage",
    "preversion": "npm run wrap",
    "prepublish": "npm test"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "dependencies": {
    "async": "^1.2.0",
    "chalk": "^1.0.0",
    "commander": "^2.8.1",
    "css": "^2.2.0",
    "css-selector-parser": "^1.0.4",
    "glob": "^5.0.6",
    "jade-lexer": "benedfit/jade-lexer",
    "jade-parser": "^0.0.8",
    "jade-walk": "^0.0.2",
    "jsonminify": "^0.2.3",
    "lodash": "^3.1.0",
    "minimatch": "^2.0.1",
    "source-map": "^0.4.1",
    "source-map-resolve": "^0.3.1",
    "update-notifier": "^0.5.0"
  },
  "devDependencies": {
    "istanbul": "^0.3.10",
    "jscs": "^1.9.0",
    "jshint": "^2.5.5",
    "mocha": "^2.2.5",
    "pliers": "^1.2.1",
    "pliers-clean-shrinkwrap": "^1.0.1",
    "stylus": "^0.51.0"
  }
}
