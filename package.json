{
  "name": "stylperjade",
  "version": "0.3.4",
  "description": "Checks Jade for unused CSS classes",
  "author": "Ben Edwards <hello@benedfit.com>",
  "license": "ISC",
  "keywords": [
    "uncss",
    "unusedcss",
    "optimize",
    "optimise",
    "unused",
    "selector",
    "sourcemap",
    "css",
    "stylus",
    "jade",
    "html"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:benedfit/stylperjade"
  },
  "publishConfig": {
    "registry": "http://registry.npmjs.org"
  },
  "main": "lib/stylperjade",
  "scripts": {
    "lint": "jshint .",
    "checkStyle": "jscs .",
    "pretest": "npm run lint && npm run checkStyle",
    "test": "istanbul cover _mocha -- -R spec",
    "posttest": "istanbul check-coverage && rm -rf coverage",
    "prepublish": "npm test && npm prune"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "dependencies": {
    "async": "^0.9.0",
    "chalk": "^0.5.1",
    "css": "^2.2.0",
    "css-selector-parser": "^1.0.4",
    "jade-lexer": "0.0.3",
    "jade-parser": "0.0.2",
    "jade-walk": "git://github.com/benedfit/jade-walk.git",
    "lodash": "^3.1.0",
    "minimatch": "^2.0.1",
    "node-json-minify": "^0.1.3-a",
    "source-map": "^0.4.1",
    "source-map-resolve": "^0.3.1"
  },
  "devDependencies": {
    "glob": "^4.4.0",
    "istanbul": "^0.3.10",
    "jscs": "^1.9.0",
    "jshint": "^2.5.5",
    "mocha": "^1.21.4",
    "stylus": "^0.50.0"
  }
}
