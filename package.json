{
  "name": "stylperjade",
  "version": "1.2.3",
  "description": "Checks Jade against CSS, and vice versa, for unused and blacklisted classes",
  "author": "Ben Edwards <hello@benedfit.com>",
  "license": "ISC",
  "keywords": [
    "uncss",
    "unusedcss",
    "optimize",
    "optimise",
    "unused",
    "selector",
    "sourcemap",
    "css",
    "stylus",
    "jade",
    "html"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:benedfit/stylperjade"
  },
  "publishConfig": {
    "registry": "http://registry.npmjs.org"
  },
  "main": "./lib/stylperjade",
  "bin": {
    "stylperjade": "./bin/stylperjade"
  },
  "scripts": {
    "wrap": "rm -f npm-shrinkwrap.json; npm prune && npm install && npm shrinkwrap && pliers cleanShrinkwrap",
    "test": "./node_modules/.bin/istanbul --include-all-sources cover ./node_modules/.bin/_mocha -- -R spec",
    "posttest": "./node_modules/.bin/istanbul check-coverage && rm -rf coverage",
    "postversion": "npm run wrap",
    "prepublish": "npm test"
    "lint": "eslint -f unix .",
    "inspect": "jsinspect",
    "pretest": "npm run lint && npm run inspect",
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "dependencies": {
    "async": "^1.2.0",
    "chalk": "^1.0.0",
    "commander": "^2.8.1",
    "css": "^2.2.0",
    "css-selector-parser": "^1.0.4",
    "glob": "^5.0.6",
    "jade-lexer": "^0.0.7",
    "jade-parser": "^0.0.8",
    "jade-walk": "^0.0.2",
    "jsonminify": "^0.2.3",
    "lodash": "^3.1.0",
    "minimatch": "^2.0.1",
    "multi-glob": "^0.4.0",
    "source-map": "^0.4.1",
    "source-map-resolve": "^0.3.1"
  },
  "devDependencies": {
    "istanbul": "^0.3.10",
    "eslint": "^1.8.0",
    "eslint-config-clock": "^1.0.0",
    "eslint-config-standard": "^4.3.2",
    "eslint-plugin-standard": "^1.3.0",
    "jsinspect": "^0.7.0",
    "mocha": "^2.2.5",
    "pliers": "^1.2.1",
    "pliers-clean-shrinkwrap": "^1.0.1",
    "stylus": "^0.51.0"
  }
}
