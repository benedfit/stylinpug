{
  "name": "stylperjade",
  "version": "1.2.6",
  "description": "Checks Jade against Stylus, and vice versa, for unused and blacklisted classes",
  "author": "Ben Edwards <hello@benedfit.com>",
  "license": "ISC",
  "keywords": [
    "uncss",
    "unusedcss",
    "unusedstylus",
    "optimize",
    "optimise",
    "unused",
    "selector",
    "sourcemap",
    "css",
    "stylus",
    "jade",
    "pug",
    "html"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:benedfit/stylperjade"
  },
  "publishConfig": {
    "registry": "http://registry.npmjs.org"
  },
  "main": "./lib/reporter",
  "bin": {
    "stylperjade": "./bin/stylperjade"
  },
  "scripts": {
    "lint": "eslint -f unix .",
    "inspect": "jsinspect",
    "depcheck": "david --dev",
    "coverage": "istanbul cover ./node_modules/.bin/_mocha test",
    "pretestSkip": "npm run lint && npm run inspect && npm run depcheck",
    "test": "npm run coverage",
    "posttest": "(istanbul check-coverage --statements 90 --branches 90 --functions 100 --lines 90 && rimraf coverage) || echo Look at 'coverage/lcov-report/index.html' to find out more"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "dependencies": {
    "async": "^1.2.0",
    "chalk": "^1.0.0",
    "commander": "^2.8.1",
    "jade-lexer": "^0.0.9",
    "lodash": "^4.13.1",
    "minimatch": "^3.0.0",
    "multi-glob": "^1.0.1",
    "pug-lexer": "^2.0.2",
    "strip-json-comments": "^2.0.1",
    "stylus": "^0.54.5"
  },
  "devDependencies": {
    "david": "^7.0.0",
    "eslint": "^2.11.1",
    "eslint-config-clock": "^1.0.2",
    "eslint-config-standard": "^5.3.1",
    "eslint-plugin-promise": "^1.3.2",
    "eslint-plugin-standard": "^1.3.1",
    "istanbul": "^0.4.1",
    "jsinspect": "^0.8.0",
    "mocha": "^2.2.5",
    "rimraf": "^2.5.2"
  }
}
